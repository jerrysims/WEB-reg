<% @student_forms.each do |sf_ary| %>
  <% s = sf_ary[0] %>
  <% mf = sf_ary[1] %>
  <div class="row box student-forms-box">
    <div class="col-4">
      <h6><%= s.full_name %></h6>
    </div>
    <div class="col-8">
      <% if mf.present? %>
        <div class="row enrollment-forms-box-row">
          <div class="col-md-1">
            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-check-square" viewBox="0 0 16 16">
              <path d="M14 1a1 1 0 0 1 1 1v12a1 1 0 0 1-1 1H2a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1h12zM2 0a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2H2z"/>
              <path d="M10.97 4.97a.75.75 0 0 1 1.071 1.05l-3.992 4.99a.75.75 0 0 1-1.08.02L4.324 8.384a.75.75 0 1 1 1.06-1.06l2.094 2.093 3.473-4.425a.235.235 0 0 1 .02-.022z"/>
            </svg>      
          </div>
          <div class="col-md-6">
            <%= "Medical Form" %>
          </div>
            <div class="col-md-2">
              <%= link_to(
                "View", 
                parent_registration_period_student_medical_form_path(
                  parent_id: current_parent.id, 
                  registration_period_id: @rp.id,
                  student_id: s.id,
                  id: mf.id
                ) 
              )%>
            </div>
            <div class="col-md-2">
              <%= link_to( 
                "Edit", 
                edit_parent_registration_period_student_medical_form_path(
                  parent_id: current_parent.id, 
                  registration_period_id: @rp.id,
                  student_id: s.id,
                  id: mf.id
                ) 
              ) %>
            </div>
        </div>
      <% else %>
        <div class="row enrollment-forms-box-row">
          <div class="col-md-1">
            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-square" viewBox="0 0 16 16">
              <path d="M14 1a1 1 0 0 1 1 1v12a1 1 0 0 1-1 1H2a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1h12zM2 0a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2H2z"/>
            </svg>
          </div>
          <div class="col-md-9">
            <%= link_to(
              "Medical Form - Start Now", 
              new_parent_registration_period_student_medical_form_path(
                parent_id: current_parent.id,
                student_id: s.id,
                registration_period_id: @rp.id
              )
            ) %>
          </div>
        </div>
      <% end %>
    </div>
  </div>
    <%
=begin%>
 <% if s.needs_learning_differences_link %> 
      <div class="row enrollment-forms-box-row">
        <% if s.learning_differences_form.present? %>
          <div class="col-md-1">
            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-check-square" viewBox="0 0 16 16">
              <path d="M14 1a1 1 0 0 1 1 1v12a1 1 0 0 1-1 1H2a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1h12zM2 0a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2H2z"/>
              <path d="M10.97 4.97a.75.75 0 0 1 1.071 1.05l-3.992 4.99a.75.75 0 0 1-1.08.02L4.324 8.384a.75.75 0 1 1 1.06-1.06l2.094 2.093 3.473-4.425a.235.235 0 0 1 .02-.022z"/>
            </svg>      </div>
          <div class="col-md-9">"Learning Differences Form"</div>
        <% else %>
          <div class="col-md-1">
            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-square" viewBox="0 0 16 16">
              <path d="M14 1a1 1 0 0 1 1 1v12a1 1 0 0 1-1 1H2a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1h12zM2 0a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2H2z"/>
            </svg>
          </div>
          <div class="col-md-9">
            <%= link_to "Learning Differences Form - Click to Begin", new_student_learning_differences_form_path(s.id) %>
          </div>
        <% end %>
      </div>  
    <% end %> 
<%
=end%>
<% end %>