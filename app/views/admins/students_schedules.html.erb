<h2>View Student Schedule</h2>
<div class="row">
  <div class="col-md-6">
    <%= form_with(url: admin_student_schedule_path, local: true) do |f| %>
      <%= f.label :Student, "Search for:" %>
      <%= f.collection_select :student_id, Student.enrolled(@rp).order(:last_name), :id, :full_name %>
      
      <%= f.label :registration_period, "Search for:" %>
      <%= f.collection_select :registration_period_id, RegistrationPeriod.all, :id, :name %>
      <%= f.submit "Search" %>
    <% end %>
  </div>
</div>